<?xml version="1.0" encoding="UTF-8" ?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:p="http://www.springframework.org/schema/p"
       xmlns:context="http://www.springframework.org/schema/context"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
        http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd
        http://www.springframework.org/schema/context
        http://www.springframework.org/schema/context/spring-context.xsd">

    <!-- This is our bean using setter methods -->
    <bean id="student1" class="com.example.Student">
        <property name="studentID" value="22344"/>
        <property name="studentName" value="Agrim Sigdel"/>
        <property name="studentAddress" value="Kalanki"/>
    </bean>

</beans>
